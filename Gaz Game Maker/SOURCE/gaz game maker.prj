  integer width,height,count,y
  object dib2
  width=dib[DibWidth]
  height=dib[DibHeight]
  y=0
  count=1
  make_backup()
  if strip_mode=0 then
  	for i=1 to strip_number_images do
  	if count>strip_images_per_row then
  		count=1
  		y+=image_height
  	end if
  	dib2=newDib(image_width,image_height)
